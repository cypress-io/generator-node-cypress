{
  "name": "generator-node-cypress",
  "description": "Yeoman generator for PUBLIC Node packages from Cypress.io team",
  "main": "app/index.js",
  "version": "0.0.0-development",
  "scripts": {
    "test": "npm run unit",
    "commit": "commit-wizard",
    "pretest": "npm run lint",
    "unit": "mocha --timeout 5000 app/*-spec.js",
    "pretty": "prettier-eslint --write 'app/*.js' 'test/*.js'",
    "prelint": "npm run pretty",
    "lint": "eslint --fix app/*.js test/*.js",
    "size": "t=\"$(npm pack .)\"; wc -c \"${t}\"; tar tvf \"${t}\"; rm \"${t}\";",
    "issues": "git-issues",
    "semantic-release": "semantic-action pre && npm publish && semantic-action post",
    "e2e": "./test/e2e.sh",
    "ban": "ban",
    "license": "license-checker --production --onlyunknown --csv",
    "deps": "deps-ok && dependency-check --no-dev .",
    "unused-deps": "dependency-check --unused --no-dev ."
  },
  "files": [
    "app/*",
    "!app/*-spec.js"
  ],
  "preferGlobal": true,
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/cypress-io/generator-node-cypress.git"
  },
  "keywords": [
    "yeoman",
    "generator",
    "node",
    "npm"
  ],
  "author": "Gleb Bahmutov <gleb@cypress.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cypress-io/generator-node-cypress/issues"
  },
  "homepage": "https://github.com/cypress-io/generator-node-cypress#readme",
  "dependencies": {
    "check-more-types": "2.24.0",
    "debug": "4.0.1",
    "fixpack": "2.3.1",
    "git-remote-origin-url": "2.0.0",
    "got": "7.1.0",
    "lazy-ass": "1.6.0",
    "lodash": "4.17.11",
    "parse-github-repo-url": "1.4.1",
    "yeoman-generator": "1.1.1"
  },
  "config": {
    "pre-git": {
      "commit-msg": "simple",
      "pre-commit": [
        "npm run deps",
        "npm run test",
        "npm run ban"
      ],
      "pre-push": [
        "npm run unused-deps",
        "npm run e2e",
        "npm run license",
        "npm run ban -- --all",
        "npm run size"
      ],
      "post-commit": [],
      "post-merge": []
    }
  },
  "devDependencies": {
    "ban-sensitive-files": "1.9.0",
    "dependency-check": "2.9.1",
    "deps-ok": "1.4.1",
    "eslint": "4.15.0",
    "eslint-plugin-cypress-dev": "1.1.2",
    "eslint-plugin-mocha": "4.11.0",
    "git-issues": "1.3.1",
    "github-post-release": "1.13.1",
    "license-checker": "15.0.0",
    "mocha": "5.2.0",
    "nock": "10.0.0",
    "pre-git": "3.17.1",
    "prettier-eslint-cli": "4.7.1",
    "semantic-action": "1.1.6",
    "sinon": "4.0.2",
    "snap-shot-it": "5.0.1"
  },
  "release": {
    "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {
          "type": "break",
          "release": "major"
        }
      ]
    },
    "generateNotes": "github-post-release"
  }
}
